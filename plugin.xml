        <source-file src="src/android/pojo/PlanMortalityTable.java" target-dir="src/cordova/plugin/projection/Projection/pojo" />
        <source-file src="src/android/pojo/PlanSvTable.java" target-dir="src/cordova/plugin/projection/Projection/pojo" />
        <source-file src="src/android/pojo/PremAllocTable.java" target-dir="src/cordova/plugin/projection/Projection/pojo" />
        <source-file src="src/android/pojo/oProjection.java" target-dir="src/cordova/plugin/projection/Projection/pojo" />
